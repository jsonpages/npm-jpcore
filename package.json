{
  "name": "jsonpages-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev -w tenant-alpha",
    "build": "npm run build -w tenant-alpha",
    "build:all": "npm run build --workspaces --if-present",
    "release": "node scripts/release.js"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "typescript": "^5.7.3"
  }
}